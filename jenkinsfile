pipeline {

  agent any
  stages {
    stage('Cloning Git') {
      steps {
        git  'https://github.com/Umair841/ECS-pipeline.git'
      }
    }
    stage('Deploy Image') {
      steps{
         script {
              stage('docker build/push') {
                 docker.withRegistry('https://index.docker.io/v1/', 'Docker') {
                 def app = docker.build("umair841/demo:${commit_id}", '.').push()
          }
        }
      }
    }
